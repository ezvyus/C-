#include <iostream>
using namespace std;

int main() {
	int sayi1, sayi2, sayi3;
	cout << "Birinci sayiyi giriniz : " << endl;
	cin >> sayi1;
	cout << "Ikinci sayiyi giriniz : " << endl;
	cin >> sayi2;
	cout << "Ucuncu sayiyi giriniz : " << endl;
	cin >> sayi3;
	cout << "Girdiginiz sayilarin toplami : " << sayi1 + sayi2 + sayi3 << endl;
	return 0;
}
