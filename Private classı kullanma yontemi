#include <iostream>
using namespace std;

class Employee {
private:
	string name;
	int age;
public:
	void setAge(int yas) {
		age = yas;
	}
	int getAge(){
		return age;
	}
	void setName (string isim) {
		name = isim;
	}
	string getName() {
		return name;
	}
};

int main() {
	Employee employee;

	employee.setAge(17);
	employee.setName("Hakan");

	cout << "Kişinin yaşı : " << employee.getAge() << endl;
	cout << "Kişinin Adı : " << employee.getName() << endl;

	return 0;
}
